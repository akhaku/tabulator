{
  "manifest_version": 2,

  "name": "Tabulator",
  "author": "Ammar Khaku",
  "version": "1.1.0",
  "description": "Manage your open tabs from the new tab page",
  "icons": {
    "128": "images/icon-128.png"
  },

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "background": {
    "scripts": ["bundles/background.js"],
    "persistent": true
  },

  "permissions": [
    "tabs",
    "<all_urls>"
  ],

  "minimum_chrome_version": "50"
}
